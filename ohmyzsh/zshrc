# COS DB SETUP

POSTGRES_ENVS="prod-va6 prod-va6c2 prod-irl1 prod-jpn3 stage-va6 stage-va6c2 stage-irl1 stage-jpn3 dev-va6 dev-va6c2 dev-irl1 dev-jpn3"
function postgresSshTunnel() {
  [ $# -ne 2 ] && { echo "Usage: postgresSshTunnel prod-va6 shard02"; return; }
  ssh -A $(whoami)@cos-$1-postgres-bastion.adobecos.com@scb.dmz.ut1.adobe.net -L 5432:cos-$1-$2-aurora-postgres-ro.adobecos.com:5432 PORT-FORWARD
}
compdef "compadd ${POSTGRES_ENVS}; compadd shard01 shard02" postgresSshTunnel

# END COS DB SETUP

