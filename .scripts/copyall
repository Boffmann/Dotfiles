cp -r /home/hendrik/.scripts/* /home/hendrik/Documents/Repositories/Dotfiles/.scripts
cp /home/hendrik/.vimrc /home/hendrik/Documents/Repositories/Dotfiles/rc
cp /home/hendrik/.bashrc /home/hendrik/Documents/Repositories/Dotfiles/rc
cp /home/hendrik/.config/dunst/dunstrc /home/hendrik/Documents/Repositories/Dotfiles/rc
cp /home/hendrik/.zshrc /home/hendrik/Documents/Repositories/Dotfiles/rc
